        }

        .export {
            background: #fff;
            color: #111;
            font-weight: 300;
        }

        .export b {
            font-weight: 400;
        }

        .export table {
            font-size: 16px;
        }

        .export table th {
            font-weight: 400;
        }

        .export .mt {
            margin-top: 20px;
        }

        .export .export-pitch {
            background-color: #1D2429;
            background-position: center center;
            background-repeat: no-repeat;
            background: radial-gradient(circle, #242F3A, #1D2429);
            width: 500px;
            height: 750px;
            position: relative;
            margin: 40px auto 20px;
        }

        .export .export-pitch:before {
            background: inherit;
            position: absolute;
            top: -20px;
            right: -20px;
            bottom: -20px;
            left: -20px;
            content: '';
            display: block;
        }

        .export .export-pitch .pitch-svg {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }

        .export .export-pitch .pitch-area rect,
        .export .export-pitch .pitch-area line,
        .export .export-pitch .pitch-area circle {
            fill: none;
            stroke: #fff;
            stroke-width: 2px;
        }

        .export .export-pitch .pitch-area rect,
        .export .export-pitch .pitch-area line {
            shape-rendering: crispEdges;
        }

        .export .export-pitch .pitch-player {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            position: absolute;
            text-align: center;
            line-height: 40px;
            font-weight: 400;
            font-size: 24px;
            margin: -20px 0 0 -20px;
        }

        .export .export-pitch .pitch-player.home_team {
            background: #fff;
        }

        .export .export-pitch .pitch-player.quest_team {
            background: #32cf65;
        }

        .export .player-profile {
            margin: 40px 0;
        }

        .export .player-profile + .player-profile {
            margin-top: 60px;
        }

        .export .export-title {
            margin-top: 180px;
        }

        .export .export-comment-title {
            margin-top: 20px;
            padding-bottom: 5px;
        }

        .break-after {
            page-break-after: always;
        }

    </style>
</head>
<body>

    <div class="export">

        <div class="break-after">
            <div class="export-title"></div>
            <h2 class="text-center"><%= home_team.team_name %> – <%= quest_team.team_name %></h2>
            <div class="mt">
                <div><b>Date:</b> <%= times.date %></div>
                <div><b>Time:</b> <%= times.time %></div>
                <div><b>Location:</b> <%= home_team.stadion_name %>, <%= location %></div>
            </div>

            <% if (comment) { %>
                <div class="export-comment-title"><b>Comment:</b></div>
                <div><%- comment %></div>
            <% } %>

            <table>
                <caption>Teams comment</caption>
                <thead>
                <tr>
                    <th><%= home_team.team_name %></th>
                    <th><%= quest_team.team_name %></th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td><%- home_team.note %></td>
                    <td><%- quest_team.note %></td>
                </tr>
                </tbody>
            </table>
        </div>

        <div class="break-after">
            <div class="export-pitch">
                <svg class="pitch-svg">
                    <g class="pitch-area">
                        <defs>
                            <clipPath id="cut">
                                <rect x="1" y="120.67999999999995" width="498" height="508.63999999999976" fill="red"></rect>
                            </clipPath>
                        </defs>
                        <rect x="1" y="1" width="498" height="747.9999999999997"></rect>
                        <rect x="100.6" y="1" width="298.8" height="119.67999999999995"></rect>
                        <rect x="100.6" y="629.3199999999997" width="298.8" height="119.67999999999995"></rect>
                        <rect x="175.3" y="1" width="149.4" height="44.87999999999998"></rect>
                        <rect x="175.3" y="704.1199999999997" width="149.4" height="44.87999999999998"></rect>
                        <line x1="1" x2="499" y1="374.99999999999983" y2="374.99999999999983"></line>
                        <circle cx="250" cy="374.99999999999983" r="69.72000000000001"></circle>
                        <circle cx="250" cy="83.27999999999996" r="69.72000000000001" clip-path="url(#cut)"></circle>
                        <circle cx="250" cy="666.7199999999997" r="69.72000000000001" clip-path="url(#cut)"></circle>
                    </g>
                </svg>

                <% for( var a = 0; a < home_team.items.length; a++) {%>
                    <% if (home_team.items[a].active) { %>
                        <div class="pitch-player home_team" style="left: <%= home_team.items[a].offset.left * 100 %>%; top: <%= home_team.items[a].offset.top * 100 %>%;"><%= home_team.items[a].number %></div>
                    <% } %>
                <% } %>

                <% for( var b = 0; b < quest_team.items.length; b++) {%>
                    <% if (quest_team.items[b].active) { %>
                        <div class="pitch-player quest_team" style="left: <%= quest_team.items[b].offset.left * 100 %>%; top: <%= quest_team.items[b].offset.top * 100 %>%;"><%= quest_team.items[b].number %></div>
                    <% } %>
                <% } %>

            </div>
        </div>

        <div class="break-after">
            <h3 class="mt"><%= home_team.team_name %> players</h3>
            <table>
                <thead>
                    <tr>
                        <th width="50" class="text-right">№</th>
                        <th width="70%">Name</th>
                        <th width="30%">Position</th>
                    </tr>
                </thead>
                <tbody>
                    <% for( var c = 0; c < home_team.items.length; c++) {%>
                        <tr>
                            <td class="text-right"><%= home_team.items[c].number %></td>
                            <td><%= home_team.items[c].name %></td>
                            <td><%= home_team.items[c].position %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>

        <div class="break-after">
            <h3 class="mt"><%= quest_team.team_name %> players</h3>
            <table>
                <thead>
                <tr>
                    <th width="50" class="text-right">№</th>
                    <th width="70%">Name</th>
                    <th width="30%">Position</th>
                </tr>
                </thead>
                <tbody>
                    <% for( var d = 0; d < quest_team.items.length; d++) {%>
                        <tr>
                            <td class="text-right"><%= quest_team.items[d].number %></td>
                            <td><%= quest_team.items[d].name %></td>
                            <td><%= quest_team.items[d].position %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>

        <% for( var e = 0; e < home_team.groups.length; e++) {%>
            <div class="break-after">
                <% if (e === 0) { %>
                    <h2><%= home_team.team_name %></h2>
                <% } %>

                <% for( var f = 0; f < home_team.groups[e].length; f++) {%>
                    <div class="player-profile">
                        <h2><%= home_team.groups[e][f].name %></h2>
                        <hr>
                        <div class="mt">
                            <div><b>Number: </b><%= home_team.groups[e][f].number %></div>
                            <div><b>Position: </b><%= home_team.groups[e][f].position %></div>
                        </div>
                        <% if (home_team.groups[e][f].values[3].value && home_team.groups[e][f].values[3].value.length) { %>
                            <div class="mt">
                                <div><b>Comment:</b></div>
                                <div><%= home_team.groups[e][f].values[3].value %></div>
                            </div>
                        <% } %>
                        <table>
                            <thead>
                            <tr>
                                <th width="20%">Goal</th>
                                <th width="20%">Pass</th>
                                <th width="20%">Tackle</th>
                                <th width="20%">Technique</th>
                                <th width="20%">Speed</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <% for( var g = 0; g < home_team.groups[e][f].values.length; g++) {%>
                                    <% if (home_team.groups[e][f].values[g].name !== 'comment') { %>
                                        <% if (home_team.groups[e][f].values[g].value == 0) { %>
                                            <td>--</td>
                                        <% } else if (home_team.groups[e][f].values[g].value == 1) { %>
                                            <td>-</td>
                                        <% } else if (home_team.groups[e][f].values[g].value == 2) { %>
                                            <td>0</td>
                                        <% } else if (home_team.groups[e][f].values[g].value == 3) { %>
                                            <td>+</td>
                                        <% } else if (home_team.groups[e][f].values[g].value == 3) { %>
                                        <%  %>
                                            <td>++</td>
                                        <% } else { %>
                                            <td>&nbsp;</td>
                                        <% } %>
                                    <% } %>
                                <% } %>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                <% } %>

            </div>
        <% } %>

        <% for( var h = 0; h < quest_team.groups.length; h++) {%>
            <div class="break-after">
                <% if (h === 0) { %>
                    <h2><%= home_team.team_name %></h2>
                <% } %>

                <% for( var i = 0; i < quest_team.groups[h].length; i++) {%>
                    <div class="player-profile">
                        <h2><%= quest_team.groups[h][i].name %></h2>
                        <hr>
                        <div class="mt">
                            <div><b>Number: </b><%= quest_team.groups[h][i].number %></div>
                            <div><b>Position: </b><%= quest_team.groups[h][i].position %></div>
                        </div>
                        <% if (quest_team.groups[h][i].values[3].value && quest_team.groups[h][i].values[3].value.length) { %>
                            <div class="mt">
                                <div><b>Comment:</b></div>
                                <div><%= quest_team.groups[h][i].values[3].value %></div>
                            </div>
                        <% } %>
                        <table>
                            <thead>
                            <tr>
                                <th width="20%">Goal</th>
                                <th width="20%">Pass</th>
                                <th width="20%">Tackle</th>
                                <th width="20%">Technique</th>
                                <th width="20%">Speed</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <% for( var j = 0; j < quest_team.groups[h][i].values.length; j++) {%>
                                    <% if (quest_team.groups[h][i].values[j].name !== 'comment') { %>
                                        <% if (quest_team.groups[h][i].values[j].value == 0) { %>
                                            <td>--</td>
                                        <% } else if (quest_team.groups[h][i].values[j].value == 1) { %>
                                            <td>-</td>
                                        <% } else if (quest_team.groups[h][i].values[j].value == 2) { %>
                                            <td>0</td>
                                        <% } else if (quest_team.groups[h][i].values[j].value == 3) { %>
                                            <td>+</td>
                                        <% } else if (quest_team.groups[h][i].values[j].value == 3) { %>
                                        <%  %>
                                            <td>++</td>
                                        <% } else { %>
                                            <td>&nbsp;</td>
                                        <% } %>
                                    <% } %>
                                <% } %>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                <% } %>

            </div>
        <% } %>

</body>
</html>